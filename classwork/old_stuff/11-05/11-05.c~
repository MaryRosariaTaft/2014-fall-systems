#include <stdlib.h>
#include <stdio.h>
#include <unistd.h>

int f;

int other_function(){
  printf("pid before fork: %d\n",getpid());
  f=fork();
  //fork() returns the PID of the child to the parent; and 0 (always 0) to the child
  printf("pid after fork: %d\n",getpid());
}

int main(){

  printf("pid before other_function: %d\n",getpid());
  other_function();
  printf("pid after other_function: %d\n",getpid());
  printf("f:%d\n",f);

  return 0;

}
